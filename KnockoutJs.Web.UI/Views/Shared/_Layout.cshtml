<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Scripts.Render("~/js/modernizr")
    @Styles.Render("~/css/themes/base/css")
    @Styles.Render("~/css/bootstrap")
    @Styles.Render("~/css/wijmo")
    @Styles.Render("~/css/site")
</head>
<body>
    @RenderBody()
    @Scripts.Render("~/js/jquery")
    @Scripts.Render("~/js/jqueryui")
    @Scripts.Render("~/js/bootstrap")
    @*@Scripts.Render("~/js/wijmo")*@
    <script src="@Scripts.Url("~/Scripts/jquery.wijmo-open.all.2.3.5.min.js")" type="text/javascript"></script>
    <script src="@Scripts.Url("~/Scripts/jquery.wijmo-complete.all.2.3.5.min.js")" type="text/javascript"></script>
    @Scripts.Render("~/js/knockout")
    @Scripts.Render("~/js/knockoutwijmo")
    @RenderSection("scripts", required: false)
</body>
</html>
